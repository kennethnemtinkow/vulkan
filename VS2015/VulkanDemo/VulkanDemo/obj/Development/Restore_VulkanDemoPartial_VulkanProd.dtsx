          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{79BFAA3C-BF4A-4F0F-BCB1-E333DB9FC159}"
              SQLTask:SqlStatementSource="&#xA;     &#xA;       &#xA;     &#xA;             RESTORE DATABASE [VulkanProd] &#xA;                            &#xA; FILEGROUP = 'FG2002',&#xA; FILEGROUP = 'FG2003',&#xA; FILEGROUP = 'FG2004',&#xA; FILEGROUP = 'FG2005',&#xA; FILEGROUP = 'FG2006',&#xA; FILEGROUP = 'FG2007',&#xA; FILEGROUP = 'FG2008',&#xA; FILEGROUP = 'FG2009',&#xA; FILEGROUP = 'FG2010',&#xA; FILEGROUP = 'FG2011',&#xA; FILEGROUP = 'FG2012',&#xA; FILEGROUP = 'FG2013',&#xA; FILEGROUP = 'FG2014',&#xA; FILEGROUP = 'FG2015',&#xA; FILEGROUP = 'FG2016',&#xA; FILEGROUP = 'FG2017',&#xA; FILEGROUP = 'FG2018',&#xA; FILEGROUP = 'FG2019',&#xA; FILEGROUP = 'FG2020',&#xA; FILEGROUP = 'FG2021',&#xA; FILEGROUP = 'FG2022',&#xA; FILEGROUP = 'FG2023',&#xA; FILEGROUP = 'FG2024',&#xA; FILEGROUP = 'FG2025',&#xA; FILEGROUP = 'MainDataGroup',&#xA; FILEGROUP = 'MainIndexGroup',&#xA; FILEGROUP = 'PRIMARY'&#xA;                            FROM DISK = 'F:\VulkanBackup\VulkanProd.bak'&#xA;&#xA;       WITH &#xA;       PARTIAL, &#xA;       REPLACE,&#xA;       RECOVERY,  &#xA;       &#xA; MOVE N'VulkanProd' TO N'F:\Data\VulkanProd.mdf',&#xA; MOVE N'VulkanProd_log' TO N'F:\Log\VulkanProd_log.ldf',&#xA; MOVE N'MainDataGroup' TO N'F:\Data\MainDataGroup.ndf',&#xA; MOVE N'MainIndexGroup' TO N'F:\Data\MainIndexGroup.ndf',&#xA; MOVE N'FG2009' TO N'F:\Data\VulkanProdFG2009.ndf',&#xA; MOVE N'FG2010' TO N'F:\Data\VulkanProdFG2010.ndf',&#xA; MOVE N'FG2011' TO N'F:\Data\VulkanProdFG2011.ndf',&#xA; MOVE N'FG2012' TO N'F:\Data\VulkanProdFG2012.ndf',&#xA; MOVE N'FG2013' TO N'F:\Data\VulkanProdFG2013.ndf',&#xA; MOVE N'FG2014' TO N'F:\Data\VulkanProdFG2014.ndf',&#xA; MOVE N'FG2015' TO N'F:\Data\VulkanProdFG2015.ndf',&#xA; MOVE N'FG2016' TO N'F:\Data\VulkanProdFG2016.ndf',&#xA; MOVE N'FG2017' TO N'F:\Data\VulkanProdFG2017.ndf',&#xA; MOVE N'FG2018' TO N'F:\Data\VulkanProdFG2018.ndf',&#xA; MOVE N'FG2019' TO N'F:\Data\VulkanProdFG2019.ndf',&#xA; MOVE N'FG2020' TO N'F:\Data\VulkanProdFG2020.ndf',&#xA; MOVE N'FG2021' TO N'F:\Data\VulkanProdFG2021.ndf',&#xA; MOVE N'FG2022' TO N'F:\Data\VulkanProdFG2022.ndf',&#xA; MOVE N'FG2023' TO N'F:\Data\VulkanProdFG2023.ndf',&#xA; MOVE N'FG2024' TO N'F:\Data\VulkanProdFG2024.ndf',&#xA; MOVE N'FG2025' TO N'F:\Data\VulkanProdFG2025.ndf',&#xA; MOVE N'FG2002' TO N'F:\Data\VulkanProdFG2002.ndf',&#xA; MOVE N'FG2003' TO N'F:\Data\VulkanProdFG2003.ndf',&#xA; MOVE N'FG2004' TO N'F:\Data\VulkanProdFG2004.ndf',&#xA; MOVE N'FG2005' TO N'F:\Data\VulkanProdFG2005.ndf',&#xA; MOVE N'FG2006' TO N'F:\Data\VulkanProdFG2006.ndf',&#xA; MOVE N'FG2007' TO N'F:\Data\VulkanProdFG2007.ndf',&#xA; MOVE N'FG2008' TO N'F:\Data\VulkanProdFG2008.ndf'&#xA;&#xA;;&#xA;&#xA;     &#xA;     &#xA;    " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
