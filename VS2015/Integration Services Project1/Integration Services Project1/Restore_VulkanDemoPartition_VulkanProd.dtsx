        <DTS:Executable DTS:CreationName="Microsoft.ExecuteSQLTask" DTS:DTSID="{98D10AE5-10AE-443E-89F6-9A2FCE8A8D69}" DTS:ExecutableType="Microsoft.ExecuteSQLTask" DTS:LocaleID="-1" DTS:ObjectName="Kill Connection to VulkanProd" DTS:refId="Package\Pakkens innhold\Kill Connection to VulkanProd" DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2014; © 2014 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
          <DTS:ObjectData>
            <SQLTask:SqlTaskData SQLTask:Connection="VulkanDemoTest.master" SQLTask:SqlStatementSource="&#xA;     &#xA;       &#xA;     &#xA;             DECLARE @DatabaseName AS VARCHAR(100)&#xA;&#xA;       SET @DatabaseName = 'VulkanProd'&#xA;&#xA;       USE master&#xA;&#xA;       SET NOCOUNT ON&#xA;       DECLARE @DBName varchar(50)&#xA;       DECLARE @spidstr varchar(8000)&#xA;       DECLARE @ConnKilled smallint&#xA;       SET @ConnKilled=0&#xA;       SET @spidstr = ''&#xA;       &#xA;       IF EXISTS (SELECT name FROM master.sys.databases WHERE name = @DatabaseName)&#xA;&#xA;       Set @DBName = @DatabaseName&#xA;       IF db_id(@DBName) &lt; 4&#xA;       BEGIN&#xA;       PRINT 'Connections to system databases cannot be killed'&#xA;       RETURN&#xA;       END&#xA;       SELECT @spidstr=coalesce(@spidstr,'','' )+'kill '+convert(varchar, spid)+ '; '&#xA;       FROM master..sysprocesses WHERE dbid=db_id(@DBName)&#xA;&#xA;       IF LEN(@spidstr) &gt; 0&#xA;       BEGIN&#xA;       EXEC(@spidstr)&#xA;       SELECT @ConnKilled = COUNT(1)&#xA;       FROM master..sysprocesses WHERE dbid=db_id(@DBName)&#xA;       &#xA;       END &#xA;     &#xA;     &#xA;    " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"></SQLTask:SqlTaskData>
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable DTS:CreationName="Microsoft.ExecuteSQLTask" DTS:DTSID="{9CE9CB77-4B94-4CC7-9C9F-C30761ECC423}" DTS:ExecutableType="Microsoft.ExecuteSQLTask" DTS:LocaleID="-1" DTS:ObjectName="Restore VulkanProd" DTS:refId="Package\Pakkens innhold\Restore VulkanProd" DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2014; © 2014 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
          <DTS:ObjectData>
            <SQLTask:SqlTaskData SQLTask:Connection="VulkanDemoTest.master" SQLTask:SqlStatementSource="&#xA;     &#xA;       &#xA;     &#xA;             RESTORE DATABASE [VulkanProd] &#xA;                            &#xA; FILEGROUP = 'FG2015',&#xA; FILEGROUP = 'FG2016',&#xA; FILEGROUP = 'FG2017',&#xA; FILEGROUP = 'MainDataGroup',&#xA; FILEGROUP = 'MainIndexGroup',&#xA; FILEGROUP = 'PRIMARY'&#xA;                            FROM DISK = 'W:\VulkanProd.bak'&#xA;&#xA;       WITH &#xA;       PARTIAL, &#xA;       REPLACE,&#xA;       RECOVERY,  &#xA;       &#xA; MOVE N'VulkanProd' TO N'F:\Data\VulkanProd.mdf',&#xA; MOVE N'VulkanProd_log' TO N'F:\Log\VulkanProd_log.ldf',&#xA; MOVE N'MainDataGroup' TO N'F:\Data\MainDataGroup.ndf',&#xA; MOVE N'MainIndexGroup' TO N'F:\Data\MainIndexGroup.ndf',&#xA; MOVE N'FG2016' TO N'F:\Data\VulkanProdFG2016.ndf',&#xA; MOVE N'FG2017' TO N'F:\Data\VulkanProdFG2017.ndf',&#xA; MOVE N'FG2015' TO N'F:\Data\VulkanProdFG2015.ndf'&#xA;&#xA;;&#xA;&#xA;     &#xA;     &#xA;    " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"></SQLTask:SqlTaskData>
          </DTS:ObjectData>
        </DTS:Executable>
        <DTS:Executable DTS:CreationName="Microsoft.ExecuteSQLTask" DTS:DTSID="{B5EFED0F-B750-4826-9B7B-4BA8DB3A2B31}" DTS:ExecutableType="Microsoft.ExecuteSQLTask" DTS:LocaleID="-1" DTS:ObjectName="Set VulkanProd Recovery Mode" DTS:refId="Package\Pakkens innhold\Set VulkanProd Recovery Mode" DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2014; © 2014 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1">
